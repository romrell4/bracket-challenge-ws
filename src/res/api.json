				},
				"name": {
					"type": "string"
				}
			}
		},
		"player": {
			"properties": {
				"player_id": {
					"type": "integer"
				},
				"name": {
					"type": "string"
				}
			}
		},
		"tournament": {
			"properties": {
				"tournament_id": {
					"type": "integer"
				},
				"name": {
					"type": "string"
				},
				"master_bracket_id": {
					"type": "integer"
				}
			}
		},
		"bracket": {
			"properties": {
				"bracket_id": {
					"type": "integer"
				},
				"user_id": {
					"type": "integer"
				},
				"tournament_id": {
					"type": "integer"
				},
				"name": {
					"type": "string"
				}
			}
		},
		"match": {
			"properties": {
				"match_id": {
					"type": "integer"
