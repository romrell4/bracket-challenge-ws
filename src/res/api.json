				},
				"position": {
					"type": "integer"
				},
				"player1_id": {
					"type": "integer"
				},
				"player2_id": {
					"type": "integer"
				},
				"seed1": {
					"type": "integer"
				},
				"seed2": {
					"type": "integer"
				},
				"winner_id": {
					"type": "integer"
				}
			}
		}
	}
}